{
  "name": "rento",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "npx expo run:ios",
    "web": "expo start --web",
    "prebuild:ios": "npx expo prebuild --platform ios",
    "prebuild:android": "npx expo prebuild --platform android",
    "prebuild:clean": "expo prebuild --clean",
    "server:start": "yarn --cwd server start",
    "server:dev": "yarn --cwd server dev",
    "server:build": "yarn --cwd server build",
    "client:build": "./scripts/build-client.sh all --method=eas",
    "client:build:ios": "./scripts/build-client.sh ios --method=eas",
    "client:build:android": "./scripts/build-client.sh android --method=eas",
    "client:build:web": "./scripts/build-client.sh web",
    "client:build:local:ios": "./scripts/build-client.sh ios --method=local",
    "client:build:local:android": "./scripts/build-client.sh android --method=local",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "graphql-codegen --config codegen.yml --watch",
    "db:dev:migrate": "yarn --cwd server db:dev:migrate",
    "db:dev:status": "yarn --cwd server db:dev:status",
    "db:dev:init-appi": "yarn --cwd server db:dev:init-appi",
    "db:dev:test": "yarn --cwd server db:dev:test",
    "db:dev:up": "yarn --cwd server db:dev:up",
    "db:dev:down": "yarn --cwd server db:dev:down",
    "db:dev:restart": "yarn --cwd server db:dev:restart",
    "db:dev:logs": "yarn --cwd server db:dev:logs",
    "db:dev:psql": "yarn --cwd server db:dev:psql",
    "db:dev:clean": "yarn --cwd server db:dev:clean",
    "db:prod:migrate": "yarn --cwd server db:prod:migrate",
    "db:prod:status": "yarn --cwd server db:prod:status",
    "db:prod:init-appi": "yarn --cwd server db:prod:init-appi",
    "db:prod:test": "yarn --cwd server db:prod:test",
    "infrastructure:deploy:prod": "export AWS_PROFILE=rento-production-sso && cd infrastructure/aws/deploy && ./deploy.sh production",
    "infrastructure:deploy:dev": "export AWS_PROFILE=rento-development-sso && cd infrastructure/aws/deploy && ./deploy.sh development"
  },
  "dependencies": {
    "@apollo/client": "^4.0.7",
    "@apollo/server": "^5.0.0",
    "@as-integrations/express5": "^1.1.2",
    "@hookform/resolvers": "^5.0.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native/normalize-colors": "^0.80.0",
    "@sentry/react-native": "~6.14.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.6.0",
    "aws-amplify": "^6.15.7",
    "aws-jwt-verify": "^5.1.1",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "expo": "~53.0.8",
    "expo-auth-session": "~6.1.5",
    "expo-constants": "~17.1.6",
    "expo-crypto": "~14.1.4",
    "expo-font": "~13.3.1",
    "expo-linking": "~7.1.4",
    "expo-localization": "~16.1.5",
    "expo-router": "~5.0.6",
    "expo-secure-store": "~14.2.3",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.7",
    "expo-web-browser": "~14.1.6",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "i18next": "^25.1.3",
    "ioredis": "^5.3.2",
    "pg": "^8.16.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.56.3",
    "react-i18next": "^15.5.1",
    "react-native": "0.79.2",
    "react-native-edge-to-edge": "1.6.0",
    "react-native-nitro-modules": "0.26.2",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-unistyles": "^3.0.0-rc.5",
    "react-native-web": "^0.20.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/typescript": "^5.0.2",
    "@graphql-codegen/typescript-operations": "^5.0.2",
    "@graphql-codegen/typescript-react-apollo": "^4.3.3",
    "@graphql-codegen/typescript-resolvers": "^5.1.0",
    "@types/pg": "^8.15.5",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-native": "^5.0.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "~5.8.3"
  },
  "private": true
}
