{
  "expo": {
    "name": "rento",
    "slug": "rento",
    "scheme": "rento",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./src/assets/images/login_img.png",
    "newArchEnabled": true,
    "schema": "^[a-z][a-z0-9+.-]*$",
    "splash": {
      "image": "./src/assets/images/login_img.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.marczenn.rento"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/login_img.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.marczenn.rento"
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "react-native-edge-to-edge",
      "expo-web-browser",
      "expo-localization"
    ]
  }
}
