{
  "expo": {
    "name": "rento",
    "slug": "rento",
    "scheme": "rento",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./src/assets/images/logo_img.png",
    "newArchEnabled": true,
    "schema": "^[a-z][a-z0-9+.-]*$",
    "splash": {
      "image": "./src/assets/images/logo_img.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.marczenn.rento"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo_img.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.marczenn.rento"
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "react-native-edge-to-edge",
      "expo-web-browser",
      "expo-localization",
      ["expo-splash-screen",
        {
          "backgroundColor": "#F5F5F5",
          "image": "./src/assets/images/logo_img.png",
          "dark": {
            "image": "./src/assets/images/logo_img.png",
            "backgroundColor": "#121212"
          },
          "imageWidth": 200
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "note": "Use SENTRY_AUTH_TOKEN env to authenticate with Sentry.",
          "project": "rento-mobile",
          "organization": "rento"
        }
      ]
    ]
  }
}
